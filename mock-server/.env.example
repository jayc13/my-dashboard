# Mock Server Configuration for Third-Party Services
PORT=3001
NODE_ENV=development

# Third-Party Service Ports
GITHUB_MOCK_PORT=3002
CIRCLECI_MOCK_PORT=3003
CYPRESS_MOCK_PORT=3004
JIRA_MOCK_PORT=3005

# API Configuration
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info
LOG_API_CALLS=true

# Registry Configuration
MAX_REGISTRY_SIZE=10000
CLEAR_REGISTRY_ON_START=false

# Mock Data Configuration
ENABLE_REALISTIC_DELAYS=true
DEFAULT_DELAY_MS=100
