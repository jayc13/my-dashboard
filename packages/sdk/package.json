{
  "name": "@my-dashboard/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for My Dashboard API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && tsc-esm-fix --target=dist",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "echo \"No tests configured\"",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@my-dashboard/types": "file:../types"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": "v22.20.0"
  }
}
