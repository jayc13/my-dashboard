# Example environment variables for E2E tests
# Copy this file to .env and fill in the actual values

# API Security Key for testing (should match server configuration)
API_SECURITY_KEY=your-api-security-key

# API URL (backend server)
API_URL=http://localhost:3000

# Base URL for the application under test (frontend)
BASE_URL=http://localhost:4000

# Test configuration
TEST_TIMEOUT=30000
TEST_RETRIES=2

# Browser configuration
HEADLESS=true
SLOW_MO=0

# MySQL Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your-mysql-password
MYSQL_DATABASE=cypress_dashboard