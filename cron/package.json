{
  "name": "cron",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@my-dashboard/sdk": "file:../packages/sdk",
    "@my-dashboard/types": "file:../packages/types",
    "config": "^4.1.1",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.1",
    "luxon": "^3.7.2",
    "node-cron": "^4.2.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/config": "^3.3.0",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.6.0",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": "v22.20.0"
  }
}
