{
  "name": "cypress-dashboard-server",
  "version": "1.0.0",
  "description": "TypeScript server for Cypress dashboard data",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "migrate": "ts-node src/db/migrate.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest"
  },
  "engines": {
    "node": "v22.16.0"
  },
  "dependencies": {
    "@octokit/core": "^7.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-slow-down": "^2.0.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "helmet": "^8.1.0",
    "mysql2": "^3.14.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.17",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
